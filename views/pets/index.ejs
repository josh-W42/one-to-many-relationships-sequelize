<h1>All Pets</h1>

<div>
    <% pets.forEach(pet => { %>
        <div>
            <h2><%= pet.name %> </h2>
            <h3>Species: <%= pet.species %></h3>
            <% if (pet.description) { %>
                <p><%= pet.description %> </p>
            <% } %>
        </div>
    <% }) %>
</div>